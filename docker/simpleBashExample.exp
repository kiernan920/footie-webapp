#!/usr/bin/expect

# this is tcl regex for literal $, space, end of line
set prompt {\$ $}
spawn bash
expect -re $prompt

send "ll\n"
expect {
	"simpleBashExample.exp" { puts "found file" }
	eof { puts "End of File error" }
	timeout { puts "timeout error"}
}

send "pwd\n"
# expect your user name in the response
expect "\$USER"

close
